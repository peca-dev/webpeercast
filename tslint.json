{
    "extends": [
        "tslint:latest",
        "tslint-microsoft-contrib"
    ],
    "rules": {
        "export-name": false,
        "member-access": false,
        "missing-jsdoc": false,
        "no-any": false,
        "no-default-export": false,
        "no-for-in-array": false, // https://github.com/Microsoft/vscode-tslint/issues/70
        "no-relative-imports": false,
        "no-reserved-keywords": false,
        "restrict-plus-operands": false
    }
}